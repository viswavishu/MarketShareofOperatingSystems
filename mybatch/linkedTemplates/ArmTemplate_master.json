{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mybatch"
		},
		"AzureBatch1_accessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accessKey' of 'AzureBatch1'"
		},
		"AzureBlobStoragegen2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStoragegen2'"
		},
		"AzureDataLakeStorage1MovingFiles_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1MovingFiles'"
		},
		"Databricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'Databricks'"
		},
		"AzureBatch1_properties_typeProperties_batchUri": {
			"type": "string",
			"defaultValue": "https://unzipping.eastus.batch.azure.com"
		},
		"AzureBatch1_properties_typeProperties_poolName": {
			"type": "string",
			"defaultValue": "custom-activity-pool"
		},
		"AzureBatch1_properties_typeProperties_accountName": {
			"type": "string",
			"defaultValue": "unzipping"
		},
		"AzureDataLakeStorage1MovingFiles_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://batchdotnet.dfs.core.windows.net/"
		},
		"Databricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1220-053906-bfmylb4"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "mybatch_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStoragegen2_connectionString": {
						"value": "[parameters('AzureBlobStoragegen2_connectionString')]"
					},
					"AzureDataLakeStorage1MovingFiles_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1MovingFiles_accountKey')]"
					},
					"Databricks_accessToken": {
						"value": "[parameters('Databricks_accessToken')]"
					},
					"AzureDataLakeStorage1MovingFiles_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1MovingFiles_properties_typeProperties_url')]"
					},
					"Databricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('Databricks_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "mybatch_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBatch1_accessKey": {
						"value": "[parameters('AzureBatch1_accessKey')]"
					},
					"AzureBatch1_properties_typeProperties_batchUri": {
						"value": "[parameters('AzureBatch1_properties_typeProperties_batchUri')]"
					},
					"AzureBatch1_properties_typeProperties_poolName": {
						"value": "[parameters('AzureBatch1_properties_typeProperties_poolName')]"
					},
					"AzureBatch1_properties_typeProperties_accountName": {
						"value": "[parameters('AzureBatch1_properties_typeProperties_accountName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'mybatch_ArmTemplate_0')]"
			]
		}
	]
}